{
    "name": "rice-node-sdk",
    "version": "1.0.2",
    "description": "SDK for Rice",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/shankha98/rice-node-sdk.git"
    },
    "bugs": {
        "url": "https://github.com/shankha98/rice-node-sdk/issues"
    },
    "homepage": "https://github.com/shankha98/rice-node-sdk#readme",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./tools/anthropic": {
            "types": "./dist/tools/anthropic.d.ts",
            "default": "./dist/tools/anthropic.js"
        },
        "./tools/google": {
            "types": "./dist/tools/google.d.ts",
            "default": "./dist/tools/google.js"
        },
        "./tools/openai": {
            "types": "./dist/tools/openai.d.ts",
            "default": "./dist/tools/openai.js"
        },
        "./tools/execute": {
            "types": "./dist/tools/execute.d.ts",
            "default": "./dist/tools/execute.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc && npm run copy-proto",
        "copy-proto": "mkdir -p dist/state/proto && cp src/state/proto/state.proto dist/state/proto/state.proto",
        "prepack": "npm run build",
        "test": "jest"
    },
    "keywords": [
        "rice",
        "ai"
    ],
    "author": "",
    "license": "UNLICENSED",
    "dependencies": {
        "@grpc/grpc-js": "^1.10.0",
        "@grpc/proto-loader": "^0.7.10",
        "axios": "^1.6.0",
        "long": "^5.2.3"
    },
    "devDependencies": {
        "@types/jest": "^30.0.0",
        "@types/node": "^20.0.0",
        "dotenv": "^17.2.3",
        "jest": "^30.2.0",
        "ts-jest": "^29.4.6",
        "typescript": "^5.3.0"
    }
}